#include <stdio.h>

int main(void) {
  int num1, num2, num3;

printf("Enter first integer: ");
scanf("%d",&num1);

printf("Enter second integer: ");
scanf("%d",&num2);

printf("Enter third integer: ");
scanf("%d",&num3);

  if (num1 > num2) {
    if (num1 > num3) {
      printf("%d is the largest.\n", num1);
      if (num2 > num3) {
        printf("%d is the middle.\n", num2);
        printf("%d is the smallest.\n", num3);
      } else {
        printf("%d is the middle.\n", num3);
        printf("%d is the smallest.\n", num2);
      }
    } else {
      printf("%d is the largest.\n", num3);
      printf("%d is the middle.\n", num1);
      printf("%d is the smallest.\n", num2);
    }
  } else {
    if (num2 > num3) {
      printf("%d is the largest.\n", num2);
      if (num1 > num3) {
        printf("%d is the middle.\n", num1);
        printf("%d is the smallest.\n", num3);
      } else {
        printf("%d is the middle.\n", num3);
        printf("%d is the smallest.\n", num1);
      }
    } else {
      printf("%d is the largest.\n", num3);
      printf("%d is the middle.\n", num2);
      printf("%d is the smallest.\n", num1);
    }
  }
    if (num1 == num2) {
        printf("%d number 1 and number 2 are equal.\n")
    }
    
    if (num2 == num3) {
        printf("%d number 2 and number 3 are equal.\n")
    }
    
    if (num3 == num1) {
        printf("%d number 1 and number 3 are equal.\n")
    }
    if (num1 == num2 == num3) {
        printf("%d number 1,number 2 and number3 are equal.\n")
    }
}
